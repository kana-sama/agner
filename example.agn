reverse(List) -> reverse(List, []).
reverse([H|T], Acc) -> reverse(T, [H|Acc]);
reverse([], Acc) -> Acc.

replicate(N) -> replicate(N, []).
replicate(0, Acc) -> Acc;
replicate(N, Acc) -> replicate(N-1, [N|Acc]).

sum(List) -> sum(List, 0).
sum([H|T], Acc) -> sum(T, Acc + H);
sum([], Acc) -> Acc.

main() ->
  sum(replicate(100000000)) + sum(replicate(100000000)).